<%- include('blocks/header', {bot, user, path}) %>
<div class="container">

  <div class="jumbotron" style="background-color:#2a2d32;">
      <div class="container">
        <div class="row justify-content-md-center">
          <div class="col col-10" style="color:#fff"><h3>İstatistikler</h3>
          </div>
        </div>
        <div class="row justify-content-md-center">
          <div class="col-5 text-nowrap" style="color:#fff"><i class="fa fa-fw fa-server" aria-hidden="true" style="color:#ffffff"></i> Sunucu:</div>
          <div class="col-5 text-nowrap" style="color:#fff"><strong><%=stats.servers%></strong></div>
        </div>
        <div class="row justify-content-md-center">
          <div class="col-5 text-nowrap" style="color:#fff"><i class="fa fa-fw fa-users" aria-hidden="true" style="color:#ffffff"></i> Kullanıcı:</div>
          <div class="col-5 text-nowrap" style="color:#fff"><strong><%=stats.members%></strong></div>
        </div>
        <div class="row justify-content-md-center">
          <div class="col-5 text-nowrap" style="color:#fff"><i class="fa fa-fw fa-hashtag" aria-hidden="true" style="color:#ffffff"></i> Kanal:</div>
